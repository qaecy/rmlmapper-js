@prefix rr: <http://www.w3.org/ns/r2rml#>.
@prefix rml: <http://w3id.org/rml/>.
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#>.
@prefix fno: <https://w3id.org/function/ontology#>.
@prefix grel: <http://users.ugent.be/~bjdmeest/function/grel.ttl#>.

<TriplesMap> a rr:TriplesMap;
  rml:logicalSource <Source>;
  rr:subjectMap [ rr:constant "TestSubject" ];
  rr:predicateObjectMap [
    rr:predicateMap [ rr:constant <result> ];
    rr:objectMap [
      fnml:functionValue [
        fno:executes grel:string_match;
        grel:valueParameter [ rml:reference "businessAreaNo" ];
        grel:p_string_find [ rr:constant ".+" ];
      ]
    ];
  ].

<Source> a rml:LogicalSource;
  rml:source "input.json";
  rml:iterator "$[*]".